{
  "name": "medic-upgrade-service",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npx ts-node src/main/index.ts",
    "test": "./tests/utils/init && NODE_ENV=test ./node_modules/mocha/bin/mocha  --timeout 10000 --require ts-node/register ./tests/*.spec.ts || true && ./tests/utils/destroy || true",
    "test-on-pre-existing-cluster": "./tests/utils/init-assume-cluster-exists && NODE_ENV=test ./node_modules/mocha/bin/mocha  --timeout 10000 --require ts-node/register ./tests/*.spec.ts",
    "eslint": "eslint src/**/*.ts tests/**/*.js tests/**/*.ts",
    "coverage": "./tests/utils/init && NODE_ENV=test nyc ./node_modules/mocha/bin/mocha  --timeout 10000 --require ts-node/register ./tests/*.spec.ts || true && ./tests/utils/destroy || true",
    "e2e": "./tests/utils/init && NODE_ENV=test ./node_modules/mocha/bin/mocha  --timeout 30000 --require ts-node/register ./tests/e2e/*.spec.ts || true && ./tests/utils/destroy || true",
    "e2e-on-pre-existing-cluster": "./tests/utils/init-assume-cluster-exists && NODE_ENV=test ./node_modules/mocha/bin/mocha  --timeout 30000 --require ts-node/register ./tests/e2e/*.spec.ts",
    "build-docker-image": "docker build . -t medicmobile/upgrade-service:0.26",
    "build-docker-image-local": "docker build . -t medicmobile/upgrade-service:local"
  },
  "author": "Henok",
  "license": "ISC",
  "dependencies": {
    "@kubernetes/client-node": "^0.16.1",
    "@types/node": "^17.0.8",
    "cors": "^2.8.5",
    "express": "^4.17.3"
  },
  "devDependencies": {
    "@medic/eslint-config": "^1.1.0",
    "@types/chai": "^4.3.0",
    "@types/chai-http": "^4.2.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.0",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "chai": "^4.3.6",
    "eslint": "^8.8.0",
    "mocha": "^9.2.1",
    "nyc": "^15.1.0",
    "sinon": "^13.0.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.5"
  }
}
